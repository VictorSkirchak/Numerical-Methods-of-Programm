import numpy as np

# Задана матриця A та вектор B
A = np.array([[1, 2, -1], [3, 4, 1], [5, 1, -3]])
B = np.array([-3, 1, -2])

# Розв'язок за допомогою solve
X_solve = np.linalg.solve(A, B)

# Виведення результату
print("Розв'язок за допомогою solve:")
print("x =", X_solve[0])
print("y =", X_solve[1])
print("z =", X_solve[2])
