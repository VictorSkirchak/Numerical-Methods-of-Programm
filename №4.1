import numpy as np

# Задані матриці A і B
A = np.array([[2, -1, 1], [3, 1, -1], [0, 2, -1]])
B = np.array([[1, 0, 3], [2, 1, 1], [1, 2, 3]])

# Обчислення AB і BA
AB = np.dot(A, B)
BA = np.dot(B, A)

# Обчислення матриці C = AB - BA
C = AB - BA

# Виведення результатів
print("Матриця AB:")
print(AB)
print("\nМатриця BA:")
print(BA)
print("\nМатриця C = AB - BA:")
print(C)
